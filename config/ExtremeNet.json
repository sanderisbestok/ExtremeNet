{
    "system": {
        "dataset": "MSCOCOExtreme",
        "batch_size": 15,
        "sampling_function": "kp_detection",

        "train_split": "train",
        "val_split": "val",

        "learning_rate": 0.00025,
        "decay_rate": 10,

        "val_iter": 100,

        "opt_algo": "adam",
        "prefetch_size": 10,

        "max_iter": 1000000,
        "stepsize": 200000,
        "snapshot": 5,

        "chunk_sizes": [3,4,4,4],

        "env_variable": "TMPDIR",        

        "data_dir": "./data"
    },
    
    "db": {
        "rand_scale_min": 0.6,
        "rand_scale_max": 1.4,
        "rand_scale_step": 0.1,
        "rand_scales": null,

        "rand_crop": true,
        "rand_color": true,

        "border": 128,
        "gaussian_bump": true,

        "input_size": [511, 511],
        "output_sizes": [[128, 128]],

        "test_scales": [1],

        "top_k": 40,
        "categories": 1,
        "aggr_weight": 0.1,
        "scores_thresh": 0.1,
        "center_thresh": 0.1,
        "nms_threshold": 0.5,
        "suppres_ghost": true,

        "max_per_image": 100
    }
}
